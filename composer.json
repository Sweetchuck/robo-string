{
    "type": "robo-tasks",
    "name": "sweetchuck/robo-string",
    "description": "Exposes string manipulation methods from the symfony/string package",
    "keywords": [
        "robo-tasks",
        "string"
    ],
    "authors": [
        {
            "name": "Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "homepage": "https://github.com/Sweetchuck/robo-string",
    "support": {
        "source": "https://github.com/Sweetchuck/robo-string",
        "issues": "https://github.com/Sweetchuck/robo-string/issues"
    },
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "sweetchuck/git-hooks": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": {},
    "require": {
        "php": ">=8.2",
        "consolidation/robo": "^4.0",
        "symfony/string": "^5.0 || ^6.0"
    },
    "require-dev": {
        "ext-json": "*",
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "nuvoleweb/robo-config": "3.x-dev",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.5",
        "sweetchuck/codeception-module-robo-task-runner": "3.x-dev",
        "sweetchuck/git-hooks": "2.x-dev",
        "sweetchuck/robo-git": "3.x-dev",
        "sweetchuck/robo-phpcs": "3.x-dev",
        "sweetchuck/robo-phpstan": "2.x-dev",
        "symfony/error-handler": "^5.0 || ^6.0",
        "symfony/finder": "^5.0 || ^6.0",
        "symfony/yaml": "^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\Robo\\String\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\Robo\\String\\Tests\\Acceptance\\": "tests/acceptance/",
            "Sweetchuck\\Robo\\String\\Tests\\Unit\\": "tests/unit/",
            "Sweetchuck\\Robo\\String\\Tests\\": "tests/_support/"
        }
    },
    "scripts": {},
    "scripts-descriptions": {},
    "extra": {}
}
